{
  "name": "react-timeseries-charts",
  "version": "0.8.1",
  "description": "Declarative timeseries charts",
  "keywords": [
    "d3",
    "charts",
    "react",
    "timeseries"
  ],
  "main": "lib/index",
  "author": "ESnet Tools Team <tools@es.net>",
  "homepage": "https://software.es.net/react-timeseries-charts",
  "repository": "esnet/react-timeseries-charts",
  "bugs": {
    "url": "https://github.com/esnet/react-timeseries-charts/issues"
  },
  "scripts": {
    "prepublish": "npm test && npm run build",
    "docs": "react-docgen src/ -x jsx -o examples/docs.json --pretty",
    "lint": "eslint src/*.jsx",
    "test": "npm run lint",
    "test-only": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run docs && rm -rf lib/* && cp src/*.css lib/ && babel src --optional runtime --stage 0 --out-dir lib",
    "start-website": "webpack-dev-server  --config webpack.examples.config.js",
    "build-website": "webpack --config webpack.examples.config.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "d3": "^3.5.5",
    "d3-axis": "^0.3.0",
    "d3-format": "^0.5.1",
    "d3-selection": "^0.7.0",
    "d3-shape": "^0.6.0",
    "d3-time": "^0.2.5",
    "d3-time-format": "^0.3.1",
    "invariant": "^2.1.1",
    "merge": "^1.2.0",
    "moment": "^2.8.4",
    "moment-duration-format": "^1.3.0",
    "pondjs": "git+ssh://git@github.com/esnet/pond.git#v0.5",
    "react-flexbox": "^3.0.0",
    "underscore": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "^0.5.4",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.0",
    "eslint-config-esnet": "^0.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.8.5",
    "history": "^1.13.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-docgen": "^2.4.0",
    "react-dom": "^0.14.3",
    "react-markdown": "^1.0.5",
    "react-router": "^1.0.0",
    "scroll-behavior": "^0.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}

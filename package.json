{
  "name": "react-timeseries-charts",
  "version": "0.4.0",
  "description": "Declarative timeseries charts",
  "main": "index.js",
  "author": "ESnet Tools Team <tools@es.net>",
  "repository": {
    "type": "git",
    "url": "git://github.com:esnet/react-timeseries-charts.git"
  },
  "bugs": {
    "url": "https://github.com/esnet/react-timeseries-charts/issues"
  },
  "scripts": {
    "prepublish": "npm test && npm run build",
    "lint": "eslint src/*.jsx",
    "test": "npm run lint",
    "test-only": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib/* && cp src/*.css lib/ && babel src --optional runtime --stage 0 --out-dir lib",
    "start-website": "webpack-dev-server  --config webpack.examples.config.js",
    "build-website": "webpack --config webpack.examples.config.js",
    "deploy-website": "gh-pages-deploy"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "gh-pages-deploy": {
    "staticpath": "website",
    "prep": [
      "build-website"
    ]
  },
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "pondjs": "^0.3.x",
    "babel-runtime": "~5.8.3",
    "d3": "^3.5.5",
    "invariant": "^2.1.1",
    "merge": "^1.2.0",
    "moment": "^2.8.4",
    "paths-js": "^0.3.4",
    "underscore": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^0.14.3"
  },
  "devDependencies": {
    "babel": "^5.8.3",
    "babel-core": "^5.1.13",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.0.0",
    "bundle-loader": "^0.5.0",
    "css-loader": "^0.9.0",
    "eslint": "^1.10.0",
    "eslint-config-esnet": "^0.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.7.2",
    "gh-pages-deploy": "^0.3.0",
    "history": "^1.13.1",
    "json-loader": "^0.5.1",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-addons-clone-with-props": "^0.14.0",
    "react-dom": "^0.14.3",
    "react-markdown": "^1.0.5",
    "react-router": "^1.0.0",
    "scroll-behavior": "^0.3.0",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.5",
    "webpack-dev-server": "^1.6.5"
  },
  "keywords": [
    "d3",
    "charts",
    "react",
    "timeseries"
  ]
}
